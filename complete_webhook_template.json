  "tradingview_webhook_format": {
    "description": "Basic TradingView webhook format with available placeholders",
    "note": "Limited to basic price data and TradingView placeholders only",
    "template": {
      "action": "{{strategy.order.action}}",
      "ticker": "{{ticker}}",
      "order_size": "100%",
      "position_size": "{{strategy.position_size}}",
      "schema": "2",
      "timestamp": "{{time}}",
      "bot_id": "4vH63Kjr",
      "price_data": {
        "close": "{{close}}",
        "open": "{{open}}",
        "high": "{{high}}",
        "low": "{{low}}",
        "volume": "{{volume}}"
      }
    }
  },

